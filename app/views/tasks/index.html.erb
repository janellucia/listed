<h1>To Do</h1>
<p><%= link_to 'New Task', new_task_path, class: 'btn btn-primary' %></p>
<table>
    <tr>
        <th> Content </th>
        <th> State </th>
        <th colspan = "4"> </th>
    </tr>
    <%= render @to_do %>
</table>

<h1>Doing</h1>
<table>
    <tr>
        <th> Content </th>
        <th> State </th>
        <th colspan = "5"> </th>
    </tr>
    <%= render @doing %>
</table>


<h1>Done</h1>
<table>
    <tr>
        <th> Content </th>
        <th> State </th>
        <th colspan = "4"> </th>
    </tr>
    <%= render @done %>
</table>